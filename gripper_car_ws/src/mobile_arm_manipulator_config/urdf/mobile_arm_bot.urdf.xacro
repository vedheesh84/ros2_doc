<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="mobile_arm_bot_2">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />
    <xacro:arg name="use_gazebo" default="false" />

    <!-- Import mobile_arm_bot_2 urdf file -->
    <xacro:include filename="$(find mobile_arm_manipulator_config)/urdf/mobile_arm_bot_2.urdf.xacro" />

    <!-- Import control_xacro -->
    <xacro:include filename="mobile_arm_bot_2.ros2_control.xacro" />



</robot>
